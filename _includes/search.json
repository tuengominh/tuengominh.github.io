[
  {%- assign posts = site.posts | where_exp: "item", "item.tags contains page.target" -%}
  {%- for post in posts -%}
  {
      "title": "{{post.title | xml_escape}}",
      "meta": "{{post.meta | xml_escape}}",
      "url": "{{site.baseurl}}{{post.url}}",
      "tags": "{{post.tags}}"
  }
  {%- unless forloop.last -%},{%- endunless -%}
  {%- endfor -%}
]